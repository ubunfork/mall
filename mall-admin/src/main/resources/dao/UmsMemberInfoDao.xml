<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.dao.UmsMemberInfoDao">

    <resultMap id="memberInfoResult" type="com.macro.mall.dto.UmsMemberInfoResult" extends="com.macro.mall.mapper.UmsMemberMapper.BaseResultMap">
  
    </resultMap>

    <select id="memberInfo" resultMap="memberInfoResult">
        select *
        FROM ums_member m
        where m.id = #{id}
    </select>
</mapper>