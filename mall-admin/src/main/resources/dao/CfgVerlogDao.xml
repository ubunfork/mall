<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.dao.CfgVerlogDao">


  <select id="selectverlogplatform" resultType="Map">
    select `platform` from `cfg_verlog` group by `platform`;
  </select>

    <select id="selectverlogverstion" resultType="Map">
    select `verstion` from `cfg_verlog` 
    where `platform`=#{platform}
    group by `verstion`;
  </select>
  
</mapper>